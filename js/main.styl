    bottom: 0;
    right: 0;
    color: #ccc;
    font-family: monospace;
    font-size: small;
}

.alert-overlay {
    z-index: 100500;
    background: rgba(0, 0, 0, 0.7);
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}

.poker-table > :nth-child(1),
.poker-table > :nth-child(2) {
    -webkit-filter: hue-rotate(338deg);
    filter: hue-rotate(338deg);
}
.poker-table > :nth-child(7),
.poker-table > :nth-child(8) {
    -webkit-filter: hue-rotate(38deg);
    filter: hue-rotate(38deg);
}
.poker-table > :nth-child(49),
.poker-table > :nth-child(50) {
    -webkit-filter: hue-rotate(273deg);
    filter: hue-rotate(273deg);
}
.poker-table > :nth-child(52),
.poker-table > :nth-child(53) {
    -webkit-filter: hue-rotate(373deg);
    filter: hue-rotate(373deg);
}
.poker-table > :nth-child(55),
.poker-table > :nth-child(56) {
    -webkit-filter: hue-rotate(191deg);
    filter: hue-rotate(191deg);
}
.chess-table > .slot > sup {
    display: none;
}
.chess-table > .slot {
    background: #dbaf54;
}
.chess-table > :nth-child(-2n+8),
.chess-table > :nth-child(8) ~ :nth-child(-2n+15),
.chess-table > :nth-child(16) ~ :nth-child(-2n+24),
.chess-table > :nth-child(24) ~ :nth-child(-2n+31),
.chess-table > :nth-child(32) ~ :nth-child(-2n+40),
.chess-table > :nth-child(40) ~ :nth-child(-2n+47),
.chess-table > :nth-child(48) ~ :nth-child(-2n+56),
.chess-table > :nth-child(56) ~ :nth-child(-2n+63) {
    background: #602a0f;
}

.stats-table th {
