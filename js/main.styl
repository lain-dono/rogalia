}
#exchange-rates-table input {
    width: 40px;
    margin: 1px 3px;
}

.icon-button {
    cursor: pointer;
    padding: 2px;
    border-radius: 2px;
    background: #99682e;
    margin: 3px;
    float: right;
}
.icon-button:hover {
    -webkit-filter: brightness(150%);
    filter: brightness(150%);
}
#journal {
    width: 500px;
}
