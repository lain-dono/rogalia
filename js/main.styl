/*@import url(https://fonts.googleapis.com/css?family=Yeseva+One&subset=latin,cyrillic);*/
@import url(https://fonts.googleapis.com/css?family=Roboto:700,700italic,400italic,400&subset=latin,cyrillic);

@font-face {
    font-family: "Dejavu Sans";
    src: url("assets/fonts/DejaVuSans.ttf");
}


* {
    -webkit-user-select: none!important;
    -moz-user-select: none!important;
    user-select: none!important;
}
html, body {
    font:  13px/18px Roboto, Dejavu Sans, Sans;
    padding: 0;
    margin: 0;
    background: #7b5323;
    text-align: center;
    color: white;
    overflow: hidden;
    background: url("assets/bg/bg.png") repeat;
}
:-webkit-full-screen {
  width: 100%;
  height: 100%;
}


iframe {
    border: 0;
}

input, textarea, code, .messages, .message, .message .body, .message .from{
    -webkit-user-select: text!important;
    -moz-user-select: text!important;
    user-select: text!important;
    font:  13px/18px Roboto, Dejavu Sans, Sans;
    color: white;
}
table {
    border-collapse: collapse;
    width: 100%;
}
td, th {
    border: 1px solid #593815;
    padding: 1px 5px;
}
th {
    font-weight: normal;
    font-size: 15px;
    text-align: center;
}
label {
    display: inline-block;
}

:focus {
    box-shadow: 0 0 1px 1px #4a370c;
    outline-offset: 0;
    outline: none;
}

input[type=text], input[type=number], input[type=password], input:not([type]), textarea {
    border: 1px solid #a57031;
    padding: 0 1px;
    background: url("assets/bg/tab/send.png") no-repeat 99%, #99682e;
}
textarea {
    background-image: none;
}

::-webkit-input-placeholder { /* WebKit browsers */
    color:    #ddd;
}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
   color:    #ddd;
   opacity:  1;
}
::-moz-placeholder { /* Mozilla Firefox 19+ */
   color:    #ddd;
   opacity:  1;
}
:-ms-input-placeholder { /* Internet Explorer 10+ */
   color:    #ddd;
}

select {
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    background: #845721;
    color: #fff;
}

/* see util.dom.createInput() */
label > input[type=text], label > input[type=password] {
    margin-left: 5px;
}

button {
    color: #ffffff;
    background-color: #a47030;
    background:
    url("assets/bg/button/left.png") no-repeat left,
    url("assets/bg/button/right.png") no-repeat right,
    url("assets/bg/button/button.png") repeat-x;
    height: 18px;
    min-width: 67px;
    border: none;
    margin: 1px;
    text-decoration: none;
    border-radius: 5px;
    outline: none;
}

/* Remove button padding in FF */
button::-moz-focus-inner {
    border:0;
    padding:0;
}
button[disabled] {
    -webkit-filter: sepia(100%)!important;
    filter: sepia(100%)!important;
    color: #ccc;
}
button:hover {
    -webkit-filter: brightness(110%);
    filter: brightness(110%);
}

quote {
    color: #789922;
}

pre {
    margin: 0;
}

button:active {
    -webkit-filter: brightness(120%) contrast(130%);
    filter: brightness(120%) contrast(130%);
}

button > img {
    vertical-align: text-bottom;
    margin: 0 3px;
}
a {
    color: white;
}
a:hover {
    color: lightgreen;
}

hr {
    border: 0;
    height: 3px;
    background: #593815 url("assets/bg/hr.png") repeat-x;
    margin: 2px -5px;
    clear: both;
}
.vr {
    background: url("assets/bg/vr.png") repeat-y;
    width: 3px;
    margin: -3px 4px -3px; /* should be used between hrs */
}
.contents > .vr {
    margin: -5px 4px -5px;
}

#account {
    position: absolute;
    right: 1em;
    display: none;
}
#account span {
    font-size: large;
}
#world {
    background: #7b5323;
    background-image: url("assets/bg/loading.png");
    background-size: 100%;
    background-repeat: no-repeat;
    background-position: center;
    position: relative;
    margin: auto;
    border: 4px solid #684525;
    border-top: none;
    overflow: hidden;
}
#world.snap-left {
    margin: 0;
}
#interface {
    display: none;
}
#canvas, #interface, #map-canvas-x, #pixi {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
}
#canvas, #map-canvas-x, #pixi {
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    -o-user-select: none;
    user-select: none;
}
#menu {
    text-align: left;
    position: absolute;
    z-index: 9000!important;
}
#menu ul {
    margin: 0;
    padding: 0;
}
#menu li {
    list-style-type: none;
}
#menu .action {
    display: block;
}
#menu .action:hover {
    color: lightgreen;
    cursor: pointer;
}
#vendor > .contents {
    min-width: 250px;
}
#cursor {
    position: absolute;
    z-index: 8999;
    pointer-events: none;
}
#cursor img {
    max-width: 64px;
    max-height: 64px;
}

.cursor-hidden, .cursor-hidden * {
    cursor: none!important;
}

.main-stage #map-canvas-x {
    background: url("assets/bg/space.png") repeat;
    background-color: #333;
}
.main-stage #interface {
    display: block;
}
.exit-stage .panel {
    display: none!important;
}
.exit-stage #world {
    background: url("assets/bg/exit.png") no-repeat center bottom;
}

#fps-stats {
    margin: 10px;
    text-align: center;
}
#fps-stats > #stats {
    margin: auto;
}

.button {
    background-image: url("assets/bg/button.png");
    background-repeat: no-repeat;
    background-position: 50% 50%;
    color: white;
    width: 68px;
    height: 45px;
    line-height: 45px;
    position: relative;
    cursor: pointer;
    margin: -1px -9px;
}

.button-icon {
    font-size: x-large;
}
.button.alert:before {
    position: absolute;
    background-color: #563a11;
    box-shadow: 0 0 1px 1px #fee70f;
    border-radius: 50%;
    height: 6px;
    width: 6px;
    content: "";
    display: block;
    top: 20px;
}

.button-bar i, .button i {
    display: block;
    font-size: smaller;
    position: absolute;
    left: 0;
    right: 0;
    top: 1.25em;
}

.button-bar i:before {
    content: "(";
}
.button-bar i:after {
    content: ")";
}

#action-button:after {
    content: "(Space)";
    font-style: italic;
    font-size: smaller;
    position: absolute;
    left: 0;
    right: 0;
    bottom: -14px;
}

#warning, #messages, #anouncement {
    position: relative;
    top: 7em;
    text-align: center;
    display: none;
    color: white;
    pointer-events:none;
    z-index: 2000;
}
#warning, .info-message {
    padding: 2px 4px;
    border: 1px solid #a57031;
    border-radius: 4px;
}
#anouncement {
    background: rgba(33, 33, 33, 0.5);
    font-size: 32px;
    padding: 2em;
    position: absolute;
    left: 0;
    right: 0;
}
#messages {
    display: flex;
    flex-direction: column;
}
.info-message {
    display: inline-block;
    margin: auto;
    background: rgba(153, 104, 46, 0.7);
    margin-bottom: 5px;
}
#warning.warning {
    background: rgba(153, 104, 46, 0.7);
    color: yellow;
}
#warning.error {
    background: rgba(243, 220, 194, 0.7);
    color: red;
}


#admin-bar {
    max-width: 544px;
}
#terra-bar > .contents {
    width: 270px;
}
#terra-bar img, #map img {
    max-height: none;
    max-width: none;
}
#terra-bar img {
    position: relative;
    top: 22px;
}
#blank-panel .slot {
    float: none;
    margin: 10px auto;
}
.ingredients-list {
    padding: 0;
}
.ingredients-list > li {
    float: left;
    list-style-type: none;
    width: 64px;
    text-align: center;
}
.item-preview {
    opacity: 0.25;
}



a.extern {
    background: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAoAAAAKCAYAAACNMs+9AAAAVklEQVR4Xn3PgQkAMQhDUXfqTu7kTtkpd5RA8AInfArtQ2iRXFWT2QedAfttj2FsPIOE1eCOlEuoWWjgzYaB/IkeGOrxXhqB+uA9Bfcm0lAZuh+YIeAD+cAqSz4kCMUAAAAASUVORK5CYII=") no-repeat scroll right center transparent;
    padding-right: 13px;
}

.param {
    display: block;
}
.param > .value {
    float: right;
}
.param .meter-wrapper > img {
    width: 16px;
    height: 16px;
    position: absolute;
    top: 1px;
    left: -17px;
}

.param > .meter-wrapper {
    display: inline-block;
    width: 50%;
    position: relative;
}
meter {
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    background: #99682e;

}

meter::-webkit-meter-bar {
    background: #99682e;
}

meter::-webkit-meter-optimum-value {
    background: #ac7637;
}

meter:-moz-meter-optimum::-moz-meter-bar {
    background: #ac7637;
}

meter::-webkit-meter-suboptimum-value {
    background: #ac7637;
}

meter:-moz-meter-sub-optimum::-moz-meter-bar {
    background: #ac7637;
}

meter::-webkit-meter-even-less-good-value {
    background: #ac7637;
}

meter:-moz-meter-sub-sub-optimum::-moz-meter-bar {
    background: #ac7637;
}

.param meter {
    width: 100%;
}
.param .meter-title {
    position: absolute;
    top: 0;
    width: 100%;
    text-align: center;
    color: #fff;
}
.attr-strength, .vitamin-protein {
    color: #f99;
}
.attr-vitality, .vitamin-fat {
    color: #ff9;
}
.attr-dexterity, .vitamin-carbohydrate {
    color: #9f9;
}
.attr-intellect, .vitamin-phosphorus {
    color: #9ff;
}
.attr-perception, .vitamin-calcium {
    color: #f9f;
}
.attr-wisdom, .vitamin-magnesium {
    color: #99f;
}

#stats {
    width: 300px;
}

#stats .summary {
    background: url("assets/bg/strip/center.png") repeat-x;
    height: 32px;
    line-height: 32px;
    display: table;
    margin: auto;
    position: relative;
    text-align: center;
}

#stats .summary:before,
#stats .summary:after {
    position: absolute;
    top: -5px;
}
#stats .summary:before {
    content: url("assets/bg/strip/left.png");
    left: -60px;
}
#stats .summary:after {
    content: url("assets/bg/strip/right.png");
    right: -60px;
}
#stats .faction {
    margin-top: -7px;
    margin-bottom: -7px;
}
#stats .main {
    position: relative;
    display: flex;
    justify-content: space-around;
}
#stats .attributes, #stats .health {
    width: 50%;
}

.doll {
    position: absolute;
    width: 71px;
    height: 191px;
    text-align: center;
    margin: auto!important;
    left: 0;
    right: 0;
}

.doll.male {
    background-image: url("assets/bg/doll/male/naked.png");
}
.doll.female {
    background-image: url("assets/bg/doll/female/naked.png");
}

.equip {
    position: relative;
    height: 200px;
}

.equip > .slot {
    position: absolute;
    margin: auto;
}

.equip > .has-item {
    background-image: url("assets/bg/slot.png");
}

/* left column */
.equip-bag {
    background-image: url("assets/bg/bag.png"), url("assets/bg/slot.png");
}
.equip-neck {
    top: 50px;
    background-image: url("assets/bg/neck.png"), url("assets/bg/slot.png");
}
.equip-right-hand {
    top: 100px;
    background-image: url("assets/bg/right-hand.png"), url("assets/bg/slot.png");
}

.equip-left-hand {
    top: 150px;
    background-image: url("assets/bg/left-hand.png"), url("assets/bg/slot.png");
}
/* right column */
.equip-head {
    right: 0;
    background-image: url("assets/bg/head.png"), url("assets/bg/slot.png");
}
.equip-body {
    top: 50px;
    right: 0;
    background-image: url("assets/bg/body.png"), url("assets/bg/slot.png");
}
.equip-legs {
    top: 100px;
    right: 0;
    background-image: url("assets/bg/legs.png"), url("assets/bg/slot.png");
}
.equip-feet {
    top: 150px;
    right: 0;
    background-image: url("assets/bg/feet.png"), url("assets/bg/slot.png");
}
.ballon {
    position: absolute;
    background: #7b5323;
    z-index: 1;
    word-wrap: break-word;
    max-width: 10em;
    display: inline-block;
    padding: 0.5em;
    overflow: hidden;
    text-align: left;
    border-radius: 5px;
}

#create-character {
    text-align: center;
    width: 230px;
}
#create-character .profession-tab {
    cursor: pointer;
    padding: 3px 5px;
    border-radius: 4px 4px 0 0;
    margin-bottom: 2px;
}
#create-character .profession-tab:hover {
    -webkit-filter: brightness(150%);
    filter: brightness(150%);
}
#create-character .profession-tab.active {
    background: #73471b;
}
#create-character .profession-tabs {
    display: flex;
    flex-flow: row wrap;
    justify-content: space-around;
}
#create-character .professions {
    background: #73471b;
    border-radius: 2px;
    margin: -2px -1px 3px -1px;
    padding: 5px 7px;
    text-align: left;
}
#new-male {
    background: url("assets/avatars/new-male.png") center, url("assets/bg/main-button.png");
}
#new-female {
    background: url("assets/avatars/new-female.png") center, url("assets/bg/main-button.png");
}
.new-sex {
    background-repeat: no-repeat!important;
    width: 105px;
    height: 67px;
}
.new-sex:hover {
    -webkit-filter: brightness(110%);
    filter: brightness(110%);
}
.new-sex.checked {
    -webkit-filter: brightness(150%);
    filter: brightness(150%);
}

.new-sex input {
    display: none;
}

#login-form  {
    display: flex;
    flex-flow: column wrap;
    position: relative;
}
#login-form > label > input {
    display: block;
    margin: 0;
}

ol li{
    text-align: left;
}
ol li a:hover {
    text-decoration: underline;
    cursor: pointer;
}

.unavailable {
    color: orange;
}

.message.channel-system .body, .message.channel-server .body {
    color: #ccc;
}

#time, #maximize, #mute, #build-warning {
    position: absolute;
    right: 3px;
    top: 3px;
    border-radius: 2px;
    padding: 2px;
    cursor: pointer;
    background: #99682e;
    z-index: 2;
}
#time {
    right: 28px;
    display: none!important;
}
#maximize, #mute {
    background-position: center;
    background-repeat: no-repeat;
    width: 18px;
    height: 18px;
}
#maximize {
    background-image: url("assets/bg/maximize.png");
}
#maximize.maximized {
    background-image: url("assets/bg/minimize.png");
}
#mute {
    right: 28px;
    background-image: url("assets/bg/mute.png");
}

#mute.unmute {
    background-image: url("assets/bg/unmute.png");
}
#build-warning {
    top: 6px;
    right: 125px;
}

#build-warning-panel {
    width: 500px;
}

big {
    font-size: 24px;
}

.hidden {
    display: none!important;
}
#crash-help {
    position: absolute;
    top: 10px;
    left: 0;
    right: 0;
    width: 150px;
    margin: auto;
}
#crash-help > button {
    width: 100%;
}


.character-avatar-container {
    position: relative;
    background-image: url("assets/bg/rhombus-small.png");
    width: 103px;
    height: 60px;
}
.character-avatar-not-available {
    line-height: 60px;
    font-size: 19px;
    cursor: help;
}
.character-avatar-container > img {
    margin-left: -30px;
    width: 38px;
}

#target-container {
    position: absolute;
    left: 151px;
    top: 27px;
}
#target-container > canvas {
    margin-top: 10px;
    max-height: 40px;
    max-width: 40px;
}

#party {
    position: absolute;
    top: 85px;
}
.party-member-name {
    position: absolute;
    left: 75px;
    top: 41px;
    border-radius: 2px;
    text-shadow: 0px 0px 3px #000;
}

#minimap-container {
    right: 0;
}

#minimap {
    width: 154px;
    height: 90px;
    transform: scaleY(0.5) rotate(45deg) scale(1.5) translate(44px, 11px);
}

#minimap-canvas, #minimap-objects-canvas {
    position: absolute;
    top: 0;
    left: 0;
}
input[type=number], .lot-quantity > input, .price input {
    width: 25px;
    background-image: none;
    text-align: right;
    line-height: 15px;
    vertical-align: bottom;
}
.price {
    display: inline-block;
    text-align: right;
    white-space: nowrap;
}
.price > .silver {
    color: silver;
    margin-left: 3px;
}
.price > .gold {
    color: gold;
    margin-left: 3px;
}
.price > .platinum {
    color: white;
}
.price.negative .platinum:before {
    content: "- ";
}
.price.negative {
    border-bottom: 1px dotted red;
}

#wss {
    position: absolute;
    color: black;
    top: 50px;
    left: 50%;
    width: 200px;
    margin: 5px 0 0 -100px;
    background: white;
    z-index: 2;
    padding: 5px;
}
#wss > a{
    display: block;
    margin-top: 10px;
}

.lang-selector {
    margin: 0;
    padding: 0;
    border: none;
    position: absolute;
    right: 0;
    top: -2px;
}

#editable textarea {
    width: 300px;
    height: 250px;
    background: #99682e;
    border: none;
    border-radius: 3px;
}
#item-info {
    width: 200px;
}

#item-info .item-descr {
    padding-top: 3px;
    margin-top: 5px;
    white-space: pre-wrap;
}

#item-preview {
    float: left;
    margin-right: 5px;
    max-width: 150px;
}
.preview-wrapper {
    overflow: auto;
    float: left;
}

#news > .contents {
    max-width: 400px;
    width: 400px;
    max-height: 500px;
    overflow-y: auto;
}
#news > .contents > ul {
    margin: 0;
    padding: 0;
}

#news > .contents > ul > li {
    list-style-type: none;
}
#news li.unread > .summary {
    color: lightgreen;
}
#news .summary {
    color: #ffefd5;
    margin: 0;
    font-size: large;
    font-weight: bold;
    border-bottom: 1px solid;
}
#news .details {
    color: #eee;
    margin-top: 3px;
    margin-bottom: 10px;
}
#news .details ul {
    list-style-type: square;
}
#news time {
    float: right;
}
#talk p, #news p {
    margin: 5px 0 0;
}

#fishing  button {
    display: block;
    margin-bottom: 5px;
    width: 100%;
}
#fishing button:last-child {
    margin-bottom: 0;
}

#fishing .rating {
    text-align: center;
    padding: 7px;
}

#bank > .contents {
    text-align: right;
}
#bank label {
    text-align: left;
    display: block;
}
#bank label > span {
    float: right;
    margin-left: 5px;
}

#bank .lot-price {
    margin: 5px auto;
    display: block;
    width: auto;
}
#bank button+button {
    clear: none;
}

#action-progress-value {
    background: #ac7637;
}
#action-progress {
    background:
    url("assets/bg/action-progress/top.png") repeat-x top,
    url("assets/bg/action-progress/bottom.png") repeat-x bottom,
    #99682e;
    bottom: 170px;
    height: 23px;
    left: 0;
    box-sizing: border-box;
    display: flex;
    padding: 3px;
    margin: auto;
    position: absolute;
    right: 0;
    width: 300px;
    z-index: 3000;
}
#action-progress:before,
#action-progress:after {
    position: absolute;
    top: -2px;
}
#action-progress:before {
    content: url("assets/bg/action-progress/left.png");
    left: 0;
}
#action-progress:after {
    content: url("assets/bg/action-progress/right.png");
    right: 0;
}

#talk, #story {
    max-width: 500px;
}
.talk-link:hover {
    cursor: pointer;
    color: lightgreen;
}

#adsence-explanation {
    cursor: pointer;
    text-decoration: underline;
}
#ad-jimmy {
    display: block;
    margin: auto;
}

#adsence {
    position: fixed;
    top: 0px;
    right: 0px;
}
.claim-actions {
    width: 300px;
    height: 200px;
    position: relative;
}
.claim-action-north, .claim-action-south, .claim-action-west, .claim-action-east {
    position: absolute;
    margin: auto;
    width: 85px;
    height: 85px;
    border: 1px solid #a57031;
    background: #99682e;
}
.claim-action-north, .claim-action-south {
    right: 3px;
    left: 3px;
}
.claim-action-north {
    top: 3px;
}
.claim-action-south {
    bottom: 3px;
}
.claim-action-west, .claim-action-east {
    top: 3px;
    bottom: 3px;
}
.claim-action-west {
    left: 3px;
}
.claim-action-east {
    right: 3px;
}

